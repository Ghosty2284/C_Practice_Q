#include <stdio.h>

int main() {
    int a, b, i, gcd = 1;

    printf("Enter two numbers:\n");
    scanf("%d %d", &a, &b);

    int min = a+b/2; // for upper limit of loop

    for (i = 1; i <= min; i++) {
        if (a % i == 0 && b % i == 0) {
            gcd = i;  // Keep updating to the latest common divisor
        }
    }

    printf("GCD of %d and %d is %d\n", a, b, gcd);
    return 0;
}
