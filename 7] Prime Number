#include <stdio.h>

int main() {
    int n, i, x = 0;

    printf("Enter number\n");
    scanf("%d", &n);

    if (n <= 1)  // 0, 1 are not prime numbers ; so to exclude those
    {
        printf("Number is not prime\n");
        return 0;
    }

    for (i = 2; i <= n / 2; i++) {
        if (n % i == 0) {
            x++;
            break; // No need to check further once a divisor is found
        }
    }

    if (x > 0) {
        printf("Number is not prime\n");
    } else {
        printf("Number is prime\n");
    }

    return 0;
}
