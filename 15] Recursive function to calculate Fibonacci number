#include <stdio.h>

// Recursive function to calculate Fibonacci
int fibo(int n) {
    if (n == 0) return 0;       // Base case 1
    else if (n == 1) return 1;  // Base case 2
    else return fibo(n - 1) + fibo(n - 2); // Recursive case
}

int main() {
    int p, x;

    printf("Enter number:\n");
    scanf("%d", &p);

    x = fibo(p);
    printf("Fibonacci of %d is %d\n", p, x);

    return 0;
}
