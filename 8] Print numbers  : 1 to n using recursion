#include <stdio.h>

// Recursive function to print numbers from 1 to n
void printNumbers(int current, int n) {
    if (current > n) return; // Base case: stop when current exceeds n

    printf("%d\n", current); // Print current number
    printNumbers(current + 1, n); // Recursive call with next number
}

int main() {
    int num;

    printf("Enter number:\n");
    scanf("%d", &num);

    printNumbers(1, num); // Start printing from 1 to num

    return 0;
}
