#include<stdio.h>

int main(){
    int n,i;
    
    printf("Enter size of array :\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter array elements :\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int b[n],j;
    for(j=0;j<n;j++){
        b[j]=a[n-j-1]; // Note a travese through j=0 -> a[n-1] to j= n-1 -> a[0]
    }
    int k;
    for(k=0;k<n;k++){
        printf("%d\t",b[k]);
    }
    
}
