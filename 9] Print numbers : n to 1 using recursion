#include <stdio.h>

// Recursive function to print numbers from 1 to n
void printNumbers(int n) {
    if (n==0) return; // Base case: stop when current exceeds n

    printf("%d\n",n); // Print current number
    printNumbers( n-1); // Recursive call with next number
}

int main() {
    int num;

    printf("Enter number:\n");
    scanf("%d", &num);

    printNumbers(num); // Start printing from 1 to num

    return 0;
}
